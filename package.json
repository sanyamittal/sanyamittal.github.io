{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.10",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/sanyamittal/sanyamittal.github.io.git",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "6.x",
    "gatsby": "5.x",
    "gatsby-plugin-gatsby-cloud": "5.x",
    "gatsby-plugin-google-analytics": "5.x",
    "gatsby-plugin-manifest": "5.x",
    "gatsby-plugin-netlify": "5.x",
    "gatsby-plugin-offline": "5.x",
    "gatsby-plugin-sitemap": "5.x",
    "gatsby-plugin-webpack-bundle-analyser-v2": "5.x",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0"
  }
}
